import java.util.*;

class c {
    public <T, V> boolean bothContain(T a, V b, java.util.Collection<T> l1, java.util.Collection<V> l2) {
        boolean temp1 = false;
        boolean temp2 = true;
        for (T t : l1) { temp1 = (temp1 || t.equals(a)); }
        for (V e : l2) { temp1 = (temp1 || e.equals(b)); }
        return temp1 && temp2;
    }
    
    public c() { super(); }
}
